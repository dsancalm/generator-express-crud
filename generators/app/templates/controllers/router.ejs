import express from "express";
import { Container } from "typescript-ioc";
import { <%= modelName %>Controller } from "./<%= modelName %>Controller";

const urlPath = "/<%= modelVar %>";

export const <%= modelVar %>Router = express.Router();

const <%= modelVar %>Controller = Container.get(<%= modelName %>Controller);

<%= modelVar %>Router.post("/save<%= modelName %>", (req, res) => {
  <%= modelVar %>Controller.save<%= modelName %>(req, res);
});


<%= modelVar %>Router.get("/get<%= modelName %>ById", (req, res) => {
  <%= modelVar %>Controller.get<%= modelName %>ById(req, res);
});



<%= modelVar %>Router.delete("/delete<%= modelName %>ById", (req, res) => {
  <%= modelVar %>Controller.delete<%= modelName %>ById(req, res);
});


<%= modelVar %>Router.post("/update<%= modelName %>", (req, res) => {
  <%= modelVar %>Controller.update<%= modelName %>(req, res);
});
