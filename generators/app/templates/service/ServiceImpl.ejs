    } catch (err) {
      if (err instanceof DaoError) {
        //Do the business logic error you want...
        const error = new ServiceError(err.message, err.stack);
        return Promise.reject(error);
      } else {
        return Promise.reject(new ServiceError("Unknown error"));
      }
    }
  }


